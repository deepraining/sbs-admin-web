{
  "name": "sbs-admin-web",
  "version": "0.1.0",
  "description": "spring-boot-starter pro-admin 子项目对应的前端示例",
  "private": true,
  "author": "deepraining <jiangjinbelief@163.com>",
  "homepage": "https://github.com/deepraining/sbs-admin-web",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deepraining/sbs-admin-web"
  },
  "keywords": [
    "web",
    "spring-boot-starter"
  ],
  "bugs": {
    "url": "https://github.com/deepraining/sbs-admin-web/issues"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"./**/*.{js,vue,css,less,scss,sass,html,md,json}\"",
    "eslint": "eslint \"./**/*.js\"",
    "eslint:fix": "eslint \"./**/*.js\" --fix",
    "eslint-vue": "eslint \"./**/*.vue\" -c .eslintrc.vue.js",
    "eslint-vue:fix": "eslint \"./**/*.vue\" -c .eslintrc.vue.js --fix",
    "stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix",
    "dev": "lila dev",
    "dev:disk": "lila dev --disk",
    "build": "lila build"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "lila-bin": "^0.8.1",
    "lila-core": "^0.8.1",
    "lila-tasks": "^0.8.1",
    "lila-webpack": "^0.8.2",
    "lila-webpack-config": "^0.8.1",
    "lint-staged": "^8.1.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "prettier": "^1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^4.2.5"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.vue": [
      "eslint -c .eslintrc.vue.js --fix",
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{css,less,scss,sass}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.15.7",
    "js-cookie": "^2.2.0",
    "normalize.css": "^8.0.0",
    "nprogress": "^0.2.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  }
}
